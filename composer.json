{
    "name": "sayidmuhammad/laravel-telegram-logger",
    "description": "Send Laravel logs to Telegram chat or topic via bot",
    "keywords": ["laravel", "telegram", "logger", "logging", "monolog"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "SayidMuhammad",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "monolog/monolog": "^3.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "SayidMuhammad\\TelegramLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SayidMuhammad\\TelegramLogger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SayidMuhammad\\TelegramLogger\\TelegramLoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

